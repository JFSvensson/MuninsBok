{
  "name": "muninsbok",
  "version": "0.1.0",
  "private": true,
  "description": "Open source bokföringswebbapp för småföretag och föreningar i Sverige",
  "license": "AGPL-3.0-or-later",
  "author": "Munins bok contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/muninsbok/muninsbok"
  },
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "test": "pnpm -r test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean",
    "db:generate": "pnpm --filter @muninsbok/db generate",
    "db:migrate": "pnpm --filter @muninsbok/db migrate",
    "db:push": "pnpm --filter @muninsbok/db push"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@playwright/test": "^1.58.2",
    "@types/node": "^20.11.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
